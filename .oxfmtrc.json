{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "insertFinalNewline": true,
  "ignorePatterns": [
    "node_modules",
    ".next",
    "out",
    "build",
    "dist",
    ".wrangler",
    "coverage",
    "*.min.js"
  ]
}
